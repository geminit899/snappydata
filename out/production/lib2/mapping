APP.QUOTE.SYMBOL = ::valuelist [select q.symbol from APP.QUOTE q]

APP.ACCOUNT.ACCOUNTID = ::start 1, increment 1
APP.ACCOUNT.PROFILE_PROFILEID = APP.ACCOUNTPROFILE.PROFILEID

APP.HOLDING.HOLDINGID = ::start 1, increment 1
APP.HOLDING.QUOTE_SYMBOL = APP.QUOTE.SYMBOL
APP.HOLDING.PURCHASEPRICE < APP.QUOTE.PRICE
APP.HOLDING.ACCOUNT_ACCOUNTID = ::valuelist APP.ACCOUNT.ACCOUNTID, number 3-10

APP.ORDERS.QUOTE_SYMBOL = APP.QUOTE.SYMBOL
# APP.ORDERS.ACCOUNT_ACCOUNTID = APP.HOLDING.ACCOUNT_ACCOUNTID
APP.ORDERS.ACCOUNT_ACCOUNTID = APP.ACCOUNT.ACCOUNTID
APP.ORDERS.HOLDING_HOLDINGID = APP.HOLDING.HOLDINGID
APP.ORDERS.ORDERSTATUS = new,open,uncompleted,cancelled
APP.ORDERS.ORDERTYPE = buy,sell,short
